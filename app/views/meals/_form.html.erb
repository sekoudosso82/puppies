<%= link_to "CANCEL!!!", owners_path %>

<% if flash[:errors] %>
    <% flash[:errors].each do |e| %>
        <%= e %>
    <% end %>
<% end %>

<%= form_for @meal do |f| %>
    <%= f.hidden_field :dog_id, value: (@dog_id) %>

    <%= f.label :brand %>
    <%= f.text_field :brand %> <br>

    <%= f.label :quantity %>
    <%= f.text_field :quantity %> <br>

    <%= f.label :finished %>
    <%= f.select :finished, options_for_select([['T', true], ['F', false]])%>  <br>  

    <%= f.submit %>
<% end %>