<%= link_to "CANCEL!!!", owners_path %>

<% if flash[:errors] %>
    <% flash[:errors].each do |e| %>
        <%= e %>
    <% end %>
<% end %>

<%= form_for @owner do |f| %>
    <%= f.label :owner_image, "owner cover image" %>
    <%= f.file_field :owner_image %> <br>

    <%= f.label :first_name %>
    <%= f.text_field :first_name %> <br>

    <%= f.label :last_name %>
    <%= f.text_field :last_name %> <br>

    <%= f.label :email %>
    <%= f.text_field :email %> <br> 

    <%= f.label :phone_number %>
    <%= f.text_field :phone_number %> <br>

    <%= f.submit %>
<% end %>